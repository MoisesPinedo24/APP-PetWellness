<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcherpetbueno_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcherpetbueno_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PetWeelt"
        tools:targetApi="34">
        <activity
            android:name=".firstapp.TerminosActivity"
            android:exported="false" />
        <activity
            android:name=".firstapp.AcercaDeAppActivity"
            android:exported="false" />
        <activity
            android:name=".firstapp.Citas_notas_Activity"
            android:exported="false" />
        <activity
            android:name=".firstapp.ResultActivity"
            android:exported="false" />
        <activity
            android:name=".firstapp.RegristroActivity"
            android:exported="false" />
        <activity
            android:name=".firstapp.WolcomeActivity"
            android:exported="true"
            android:theme="@style/Theme.App.Starting">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".firstapp.LoginActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".firstapp.firstAppActivity"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>